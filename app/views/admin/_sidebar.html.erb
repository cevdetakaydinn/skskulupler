            <li class="treeview">
              <%=link_to "Kişiler", admin_users_path %>
            </li>

            <li class="treeview">
              <%=link_to "Haberler", admin_infos_path %>
            </li>
            <li>
                <%=link_to "Duyurular", admin_notices_path %>
            </li>
            <li>
                <%=link_to "Etkinlikler", admin_activities_path %>
