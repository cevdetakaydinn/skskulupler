<%= render "home/header" %>
<p id="notice"><%= notice %></p>

    <% @clubs.each do |club| %>
    <div class="panel panel-default col-md-3">
    
      <div class="panel-body">
        <%= image_tag club.avatar.url ,height: '125', width: '125' %>
      </div>
      <div class="panel-footer"><%= link_to club.name, club %></div>
    </div>
    <% end %>